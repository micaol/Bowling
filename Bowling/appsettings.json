{
  "Logging": {
    "LogLevel": {
      "Default": "Warning"
    }
  },
  "AllowedHosts": "*", 

  "docomment.activateOnEnter": true,
  "docomment.advanced": {
      "cs": {
          "namespace" : {
              "attributes" : ["summary"]
          },
          "class" : {
              "attributes" : ["summary", "typeparam"]
          },
          "interface" : {
              "attributes" : ["summary", "typeparam"]
          },
          "struct" : {
              "attributes" : ["summary"]
          },
          "enum" : {
              "attributes" : ["summary"]
          },
          "delegate" : {
              "attributes" : ["summary", "param", "typeparam", "returns"]
          },
          "field" : {
              "attributes" : ["summary"]
          },
          "property" : {
              "attributes" : ["summary", "value"]
          },
          "method" : {
              "attributes" : ["summary", "param", "typeparam", "returns"]
          },
          "event" : {
              "attributes" : ["summary"]
          }
      }
  },
  "editor.insertSpaces": true,
  "editor.tabSize": 4
}